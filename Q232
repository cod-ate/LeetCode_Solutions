// Implement Queue using Stack

class MyQueue {

    int x;
    MyQueue front, rear, next;
    public MyQueue() {}

    public MyQueue(int x) 
    {
        this.x= x;
        this.next= null;
    }   
    
    public void push(int x) 
    {
        MyQueue mq= new MyQueue(x);
        if(front==null)
            front= rear= mq;
        else
        {
            rear.next= mq;
            rear= mq;
        }
    }
    
    public int pop() 
    {
        if(front==null)
            return -1;
        int data= front.x;
        front= front.next;
        if(front==null)
            rear= null;
        return data;
    }
    
    public int peek() 
    {
        if(front==null)
            return -1;
        return front.x;
    }
    
    public boolean empty() 
    {
        return front==null; 
    }
}

/**
 * Your MyQueue object will be instantiated and called as such:
 * MyQueue obj = new MyQueue();
 * obj.push(x);
 * int param_2 = obj.pop();
 * int param_3 = obj.peek();
 * boolean param_4 = obj.empty();
 */
