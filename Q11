// Container with Most Water

class Solution {
    public int maxArea(int[] height) 
    {
        int maxCap= 0;
        int i=0, j=height.length-1;
        int min= height[0];
        while(i<j)
        {
            if(height[i] < height[j])
                min= height[i++];
            else
                min= height[j--];

            int w= j-i+1;            
            if((min * w) > maxCap)
                maxCap= min * w;
        }    
        return maxCap;
    }
}
