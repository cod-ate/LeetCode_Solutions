// Length of Last Word

class Solution {
    public int lengthOfLastWord(String s) 
    {
        s= s.trim();
        int count= 0;
        for(int i=s.length()-1; i>=0; i--)
        {
            if(s.charAt(i)!=32)
                count++;
            else
                break;
        }
        return count;
    }
}
